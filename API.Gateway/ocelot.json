{
  "GlobalConfiguration": {
    "BaseUrl": "https://localhost:9010"
  },
  "Routes": [
    {
      "UpstreamPathTemplate": "/api/gateway/contacts/{id}",
      "UpstreamHttpMethod": [ "Get" ],
      "DownstreamPathTemplate": "/api/contacts/{id}",
      "DownstreamScheme": "https",
      "DownstreamHostAndPorts": [
        {
          "Host": "localhost",
          "Port": 7108
        }
      ]
    },
    {
      "UpstreamPathTemplate": "/api/gateway/contacts?PageIndex={pageIndex}&PageSize={pageSize}",
