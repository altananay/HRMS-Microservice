      "DownstreamScheme": "https",
      "DownstreamHostAndPorts": [
        {
          "Host": "localhost",
          "Port": 7108
        }
      ]
    },
    {
      "UpstreamPathTemplate": "/api/gateway/contacts",
      "UpstreamHttpMethod": [ "Put" ],
      "DownstreamPathTemplate": "/api/contacts",
      "DownstreamScheme": "https",
      "DownstreamHostAndPorts": [
        {
          "Host": "localhost",
          "Port": 7108
        }
      ]
